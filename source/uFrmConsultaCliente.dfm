  object dstAtendimentos: TSQLDataSet
    CommandText = 
      'Select A.AGE_ID, AGE_CDLOJA, A.AGE_DTMOVI, A.AGE_HOMOVI, A.AGE_C' +
      'LIENTE,'#13#10'A.AGE_DTATENDIMENTO, A.AGE_HOATENDIMENTO, A.AGE_SITUACA' +
      'O,'#13#10'A.AGE_FUNCIONARIO, A.AGE_DESCRICAO, A.AGE_DIAGNOSTICO,'#13#10'CL.C' +
      'LI_NOME, F.FUN_NOME'#13#10'FROM AGENDA A'#13#10'inner join CLIENTES CL ON CL' +
      '.CLI_CODIGO = A.AGE_CLIENTE'#13#10'inner join FUNCIONARIOS F ON F.FUN_' +
      'CODIGO = A.AGE_FUNCIONARIO'#13#10'Where (A.age_cdloja = :pLOJA ) and (' +
      'A.AGE_CLIENTE = :pCLIENTE)'#13#10'AND (A.AGE_SITUACAO = :pSITUACAO)'
    MaxBlobSize = -1
    Params = <
